<h1> login user </h1>
<%= form_tag(:url => {:action => "login"},method: "post") do %>

<div class="field">
  <%= label_tag :email %>
  <%= text_field_tag :email%>
</div>


<div class="field">
  <%= label_tag :password %>
  <%= password_field_tag :password%>
</div>

<div class="actions">
  <%= submit_tag "login" %>
</div>
<%= link_to 'New Member', new_member_path %>
<%end%>
